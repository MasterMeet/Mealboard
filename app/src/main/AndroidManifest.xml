<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="android.Mealboard.MealBoard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/shopping_basket"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shopping_basket"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".ParthAboutActivity" />
        <activity android:name=".MeetrajAboutActivity" />
        <activity android:name=".ChintanAboutActivity" />
        <activity android:name=".MyTeamActivity" />
        <activity android:name=".OrderDetailActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".MyOrdersActivity" />
        <activity android:name=".DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".LoginActivity" />
        <activity android:name=".CartActivity" />
        <activity android:name=".ItemsDetailsActivity" />
        <activity android:name=".ItemsActivity" />
        <activity android:name=".CategoriesActivity" />

    </application>

</manifest>